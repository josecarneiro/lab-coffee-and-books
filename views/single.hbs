<h1>single shop</h1>
<h1> {{info.name}}</h1>
<span> {{info.type}}</span>

<span>{{ info.location.coordinates.[0] }}</span>
<span>{{ info.location.coordinates.[1] }}</span>


<div id="sample-map" class="map" style = "width: 1000px; height:1000px"></div>

<script>
  const $sampleMap = document.getElementById('sample-map');
  const position = {
    lat: {{ info.location.coordinates.[1] }},
    lng: {{ info.location.coordinates.[0] }}
  };
  function initMap() {
    const map = new window.google.maps.Map($sampleMap, {
      center: position,
      zoom : 9
    });
    const marker = new window.google.maps.Marker({
      position,
      map
    })
  }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBrJ1f_9MB0iFA2zFsHIbIK8sGWU91aQr8&callback=initMap"></script>


